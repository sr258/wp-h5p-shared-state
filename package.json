{
  "name": "wp-h5p-shared-state",
  "version": "0.1.0",
  "description": "A companion microservice to WordPress that offers a shared state service for H5P",
  "main": "build/index.js",
  "scripts": {
    "build": "npx tsc -P ./tsconfig.json",
    "clean": "rm -rf build && rm -rf node_modules",
    "start:watch": "ts-node-dev --deps --respawn src/index.ts",
    "start:dev": "ts-node -P ./tsconfig.json src/index.ts",
    "start": "node build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sr258/wp-h5p-shared-state.git"
  },
  "keywords": [
    "H5P",
    "Wordpress"
  ],
  "author": "Sebastian Rettig",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/sr258/wp-h5p-shared-state/issues"
  },
  "homepage": "https://github.com/sr258/wp-h5p-shared-state#readme",
  "dependencies": {
    "@lumieducation/h5p-server": "^9.0.8",
    "@lumieducation/h5p-shared-state-server": "file:../../dev/lumi/h5p-nodejs-library/packages/h5p-shared-state-server",
    "body-parser": "^1.19.1",
    "debug": "^4.3.3",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "mysql2": "^2.3.3",
    "node-fetch": "^2.6.7",
    "wp-auth": "^1.0.6"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.10",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  }
}
